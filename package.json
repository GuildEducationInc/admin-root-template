{
  "name": "guild-admin-root-pr-example",
  "author": "Guild Education Inc",
  "version": "1.0.0",
  "license": "UNLISCENSED",
  "description": "Root admin application -- guild-admin-root-pr-example",
  "engines": {
    "yarn": "^1.5"
  },
  "scripts": {
    "build:aws": "yarn build:templates && build",
    "build": "cross-env NODE_ENV=dev webpack",
    "build:templates": "node ./build-scripts/build-templates/index.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "heroku-postbuild": "yarn build",
    "heroku-prebuild": "echo \"machine github.com\n  login guildeducation\n  password ${GITHUB_TOKEN}\" > ~/.netrc",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,json,js,jsx,css}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,scss,json,js,jsx,css}\"",
    "lint": "yarn lint:script && yarn lint:style",
    "lint:script": "eslint --ext js --ext jsx --ext ts --ext tsx src",
    "lint:style": "stylelint src",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "test": "cross-env NODE_ENV=test jest --maxWorkers=2",
    "test:coverage": "jest --coverage --colors --maxWorkers=2",
    "test:e2e": "yarn start & wait-on http://localhost:8080 & cypress run",
    "test:watch": "yarn jest --watchAll"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@guildeducationinc/guild-admin-utils": "^1.0.0-beta.31",
